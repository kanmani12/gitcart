pipeline{
    agent any
    environment
    {
        PATH="/apache-maven-3.8.7-bin/apache-maven-3.8.7/bin:$PATH"
    }
        stages{
          stage('git')
            {
                steps{
                    git branch: 'main', url: 'https://github.com/kanmani12/gitcart.git'
                }
            }
            stage('maven-build')
            {
                steps{
                    mvn test
                }
            }
        }
    
}
